    <!-- Including Bootstrap JS (with its jQuery dependency) so that dynamic components work -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  </body>
  <script type="text/javascript">


    $(window).scroll(function() {
      var scroll = $(window).scrollTop();
      var x = $(window).height();

     //>=, not <=
     if (scroll >= x) {
      $(".navbar-wagon").addClass("navfixe");
      $(".navbar-wagon").removeClass("navhide");
      $("#content").addClass("navfixe1");
      $(".content1").addClass("navfixe2");
    }

    if (scroll < x) {
      $(".navbar-wagon").removeClass("navfixe");
      $(".navbar-wagon").addClass("navhide");
      $("#content").removeClass("navfixe1");
      $(".content1").removeClass("navfixe2");
    }
  });



</script>
</html>




